.main-container {
  height: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
  box-sizing: border-box;
}

header{
  height:200px;
  width:100%;
  background:#ffcc22;
}
header b{
  width:5%;
  height:20px;
  margin-top:10px;
  padding: 0 5% 0 90%;
}
header b image{
  width:24px;
  height:20px;
}

header ul{
  width:100%;
  height:100px;
  margin-left:30%;
}

header ul .head-l{
  width:100%;
  height:50px;
}
header ul .head-l image{
  display: block;
  width: 80px;
  height: 80px;
}
header ul .head-c{
  width:100%;
  height:20px;
  text-align: right;
}
header ul .head-r{
  width:100%;
  height:30px;
}
header ul .head-r image{
  display: block;
  width: 80px;
  height: 30px;
}
