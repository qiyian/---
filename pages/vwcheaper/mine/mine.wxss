.main-container {
  height: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
  box-sizing: border-box;
}

header{
  height:200px;
  width:100%;
  background:#ffcc22;
}
header b{
  display:block;
  height:20px;
  padding: 2% 0 2% 90%;
}
header b image{
  width:24px;
  height:20px;
}

header ul{
  display: block;
  height:150px;
  width:50px;
  margin-top:10px;
  padding-left:40%;
}

header ul .head-l{
  width:100%;
  height:70px;
}
header ul .head-l image{
  display: block;
  width: 70px;
  height: 70px;
}
header ul .head-c{
  display: block;
  width:100%;
  height:20px;
  margin:10px 5px;
  text-align: center;
}
header ul .head-r{
  width:100%;
  height:30px;
}
header ul .head-r image{
  display: block;
  width: 70px;
  height: 30px;
}
